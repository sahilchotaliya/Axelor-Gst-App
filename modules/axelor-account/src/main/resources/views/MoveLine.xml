<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  	https://axelor.com/xml/ns/object-views/object-views_6.0.xsd">

	<grid model="com.axelor.account.db.MoveLine" title="Move Lines"
		name="move-line-grid" canNew="false" canEdit="false">
		<field name="reference"></field>
		<field name="debit"></field>
		<field name="credit"></field>
		<field name="move"></field>
		<field name="account"></field>
	</grid>

	<form model="com.axelor.account.db.MoveLine" title="Move Line"
		name="move-line-form">
		<panel>
			<field name="reference"></field>
			<field name="account"></field>
			<field name="debit"></field>
			<field name="credit"></field>
		</panel>
	</form>
</object-views>